<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <title></title>
<#include "/jspgou_sys/head.html"/>
</head>
<body>
<div class="body-box">
    <div class="rhead">
        <div class="rpos"><@s.m "global.position"/>: 商户用户 - <@s.m "global.add"/></div>
        <form class="ropt">
            <input type="submit" value="<@s.m "global.backToList"/>" onclick="this.form.action='v_list.do';"/>
        </form>
        <div class="clear"></div>
    </div>
<#assign errorUserExist><@s.m "error.usernameExist"/></#assign>
<#assign errorEmailExist><@s.m "error.emailExist"/></#assign>
<@p.form id="jvForm" action="o-save-operator.do" labelWidth="12" method="post" enctype="multipart/form-data">
    <@p.select colspan="1" width="50" label="商家" name="commerceId" list=commerceList listKey="id" listValue="name"/>
    <@p.select colspan="1" width="50" label="店面" name="storeFront" list=storeFrontList listKey="id" listValue="storeName"  /><@p.tr/>
    <@p.checkboxlist disabled="disabled" colspan="1" width="50" valueList=roleIds  label="角色" name="roleIds" list=roleList listKey="id" listValue="name" onclick="checkedThis(this);" />
    <@p.radio colspan="1" width="50" label="admin.disabled" name="disabled" value="false" list={"true":"global.true","false":"global.false"}/><@p.tr/>
    <tr>
        <td class="pn-flabel pn-flabel-h">
            选择要导入的Excel:
        </td>
        <td class="pn-fcontent">
            <input type="file" id="file" name="file" size="20"/>
        </td>
        <td class="pn-flabel pn-flabel-h"></td>
        <td class="pn-fcontent"></td>
    </tr>
    <input type="hidden" name="viewOnlyAdmin" value="false"/>
    <@p.td colspan="2"><@p.submit code="global.submit"/> &nbsp; <@p.reset code="global.reset"/></@p.td><@p.tr/>
</@p.form>
</div>
</body>
</html>