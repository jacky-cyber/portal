.btn_k {
	height: 20px;
	margin: 15px;
	border: 1px solid #ccc;
	background-color: #6B6B6B;
	border-bottom: 1px solid #000;
	border-top: 0px;
	border-left: 0px;
	border-right: 0px;
	box-shadow: 0px 1px 0px #6a6a6a Inset;
	padding: 10px;;
	font: 18px bold;
	color: white;
	text-align: center;
}

.div_a {
	background-color: rgba(fff, fff, fff, 1);
	color: #666;
	/*text-shadow: 1px 1px 0px #000;*/
	width: 90%;
	padding: 15px 2%;
	margin: 10px 3% 0px;
	border: 1px solid #e1e0e0;
	/*box-shadow: 0px 1px 0px #e1e0e0 Inset;*/
	background-color: #FFF;
	border-radius: 3px;
}

.div_b {
	background-color: #f1f1f1;
	border: 1px solid #ced2d6;
	box-shadow: 0px 1px 0px #fff Inset;
	border-radius: 3px;
}

.inp_text {
	background-color: rgba(255, 255, 255, 1);
	height: 31px;
	color: #ccc;
	outline: none;
	padding-left: 10px;
	width: 180px;
	border: 1px solid #ebe7e8;
	box-shadow: 1px 1px 1px #f2f1f1 Inset;
	color: #666;
	outline: none;
	border-radius: 2px;
}

.inp_num {
	width: 100px;
	margin-left: 7px;
}
